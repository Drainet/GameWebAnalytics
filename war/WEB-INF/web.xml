<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WebAnalytics</servlet-name>
		<servlet-class>com.kalacool.webanalytic.WebAnalyticsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AnalyticData</servlet-name>
		<servlet-class>com.kalacool.analyticdata.AnalyticDataServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeviceDataInput</servlet-name>
		<servlet-class>com.kalacool.devicedata.input.DeviceDataInputServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebAnalytics</servlet-name>
		<url-pattern>/webanalytics</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AnalyticData</servlet-name>
		<url-pattern>/analyticdata</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeviceDataInput</servlet-name>
		<url-pattern>/devicedatainput</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
